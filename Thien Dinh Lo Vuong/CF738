n = int(input("Nhập số lượng phần tử của dãy A: "))
A = []
for i in range(n):
    x = int(input(f"Nhập phần tử thứ {i+1}: "))
    A.append(x)
dem_am = 0
for x in A:
    if x < 0:
        dem_am += 1
print("Số phần tử âm trong dãy:", dem_am)
tong_duong = 0
for x in A:
    if x > 0:
        tong_duong += x
print("Tổng các phần tử dương trong dãy:", tong_duong)
tong_3_5 = 0
for x in A:
    if x % 3 == 0 and x % 5 == 0:
        tong_3_5 += x
print("Tổng các phần tử chia hết cho 3 và 5:", tong_3_5)